<h1 class="text-2xl font-semibold mb-4">Contact Information</h1>
<p class="text-gray-700">Email: <%= @contact_info[:email] %></p>
<p class="text-gray-700">Social Media:</p>
<ul class="list-disc list-inside">
  <% @contact_info[:social_media_links].each do |link| %>
    <li><a href="<%= link[:url] %>" class="text-blue-500 hover:underline"><%= link[:platform] %></a></li>
  <% end %>
</ul>
